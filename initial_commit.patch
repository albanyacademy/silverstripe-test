+              $clearErrorBtn = this.find('.bulkUploadClearErrorButton')
+              ;
+
+          if ( total > 0 )
+          {
+            this.css({display: 'block'});
+
+            $info.html(ss.i18n.sprintf(
+              ss.i18n._t('GRIDFIELD_BULK_UPLOAD.PROGRESS_INFO'),
