+
+    <% if $Credits %>
+        <%t Blog.By "by" %>
+
+        <% loop $Credits %>
+            <% if not $First && not $Last %>, <% end_if %>
+            <% if not $First && $Last %> <%t Blog.AND "and" %> <% end_if %>
+            <% if $URLSegment %>
+                <a href="$URL">$Name.XML</a>
+            <% else %>
+                $Name.XML
+            <% end_if %>
+        <% end_loop %>
+    <% end_if %>
+</p>
