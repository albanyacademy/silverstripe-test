+
+	/**
+	 *
+	 * @return boolean
+	 */
+	public function getIsPublishJobRunning() {
+		return $this->isPublishJobRunning;
+	}
+
+	/**
+	 *
+	 * @see {@link $this->isPublishJobRunning}
+	 * @return boolean
+	 */
+	public function isPublishJobRunning() {
