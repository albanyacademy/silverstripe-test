+    public function canEditWriters($member = null)
+    {
+        $member = $this->getMember($member);
+
+        $extended = $this->extendedCan('canEditWriters', $member);
+
+        if ($extended !== null) {
+            return $extended;
+        }
+
+        if ($this->isEditor($member)) {
+            return true;
+        }
+
+        return Permission::checkMember($member, self::MANAGE_USERS);
