+    /**
+     * Returns a yearly archive link for the current blog post.
+     *
+     * @return string
+     */
+    public function getYearlyArchiveLink()
+    {
+        /**
+         * @var DBDatetime $date
+         */
+        $date = $this->dbObject('PublishDate');
