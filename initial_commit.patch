+#FeaturedImage .middleColumn {
+    clear: none;
+    float: left;
+}
+
+.has-panel .cms-content-tools.blog-admin-sidebar {
+    width: 280px;
+    border-right: none;
+    border-left: 1px solid #C0C0C2;
+    position: absolute !important; /* overrides cms !imporant style */
+    right: 0px;
+    top: 0;
+    height: 100%;
+
+    .cms-panel-toggle a {
+        text-align: left;
+        margin: 0;
+    }
+
